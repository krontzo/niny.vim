$ HelloWorld.nn
push "Hello world!\n"
dump

push [1, [2, 3, 4], 5]
flat $ stack = [1, 2, 3, 4, 5]

dump "\n"
full

macro equals do
  push "EQUALS"
  dump "\n"
  pop
end

macro other do
  push "OTHER"
  dump "\n"
  pop
end

push 5 $ stack = [5]
push 6 $ stack = [5, 6]

== $ stack = [0]
if do equals else do other
$ OUT:
$ OTHER

